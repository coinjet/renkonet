{
  "headers": [
    {
      "source": "/(.*\\.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        { "key": "Content-Type", "value": "text/css" }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/(.*\\.(woff|woff2|eot|ttf|otf))",
      "headers": [
        { "key": "Content-Type", "value": "font/" }
      ]
    }
  ]
}

